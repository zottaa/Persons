   <children>
      <ButtonBar layoutX="400.0" layoutY="360.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#xdWindow" text="XD" translateX="-10.0" fx:id="xdButton" />
            <Button fx:id="closeButton" mnemonicParsing="false" onAction="#closeWindow" text="Close" translateX="-10.0" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
