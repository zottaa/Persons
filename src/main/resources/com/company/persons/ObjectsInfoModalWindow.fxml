<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.persons.ObjectsInfoModalWindowController">
   <children>
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" onAction="#closeWindow" text="Main">
            <items>
              <MenuItem fx:id="menuCloseButton" mnemonicParsing="false" onAction="#closeWindow" text="Close" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TableView fx:id="table" layoutY="27.0" prefHeight="375.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="idColumn" editable="false" prefWidth="150.0" sortable="false" text="Id" />
          <TableColumn fx:id="typeColumn" editable="false" prefWidth="150.0" sortable="false" text="type" />
            <TableColumn fx:id="bornTimeColumn" editable="false" prefWidth="150.0" sortable="false" text="born time" />
            <TableColumn fx:id="liveTimeColumn" editable="false" prefWidth="150.0" sortable="false" text="live time" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
